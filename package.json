{
  "name": "openrelief",
  "version": "2.0.0",
  "description": "Open-source emergency coordination platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:performance": "playwright test --config=playwright.config.ts",
    "test:e2e:playwright": "playwright test --config=playwright.config.ts",
    "test:e2e:playwright:open": "playwright test --config=playwright.config.ts --ui",
    "test:e2e:playwright:debug": "playwright test --config=playwright.config.ts --debug",
    "test:e2e:playwright:report": "playwright show-report",
    "test:e2e:playwright:install": "playwright install --with-deps",
    "test:pwa": "node scripts/test-pwa.js",
    "test:lighthouse": "lhci autorun",
    "test:lighthouse:mobile": "lhci autorun --config=lighthouserc.json --ci:mobile",
    "test:lighthouse:desktop": "lhci autorun --config=lighthouserc.json --ci:desktop",
    "test:lighthouse:pwa": "lhci autorun --config=lighthouserc.json --ci:pwa-focus",
    "test:lighthouse:collect": "lhci collect --config=lighthouserc.json",
    "test:lighthouse:assert": "lhci assert --config=lighthouserc.json",
    "test:lighthouse:server": "lhci server --config=lighthouserc.json",
    "lighthouse": "node scripts/run-lighthouse.js",
    "lighthouse:dev": "node scripts/run-lighthouse.js dev",
    "lighthouse:build": "node scripts/run-lighthouse.js build",
    "lighthouse:mobile": "node scripts/run-lighthouse.js mobile",
    "lighthouse:desktop": "node scripts/run-lighthouse.js desktop",
    "lighthouse:pwa": "node scripts/run-lighthouse.js pwa",
    "lighthouse:server": "node scripts/run-lighthouse.js server",
    "db:generate": "supabase gen types typescript --local > src/types/database.ts",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "db:seed": "supabase db seed",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,css}\"",
    "prepare": "husky install",
    "analyze": "cross-env ANALYZE=true next build",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.2",
    "@mapbox/mapbox-gl-style-spec": "^14.16.1",
    "@maplibre/maplibre-gl-leaflet": "^0.0.20",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/auth-helpers-react": "^0.4.2",
    "@supabase/auth-ui-react": "^0.4.6",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.38.4",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-query-devtools": "^5.8.4",
    "@turf/bbox": "^7.3.1",
    "@turf/buffer": "^7.3.1",
    "@turf/distance": "^7.3.1",
    "@turf/helpers": "^7.3.1",
    "@turf/turf": "^7.3.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.16",
    "geolib": "^3.3.4",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.294.0",
    "mapbox-gl": "^0.28.0",
    "mapbox-gl-draw": "^0.16.0",
    "mapbox-gl-geocoder": "^2.0.1",
    "maplibre-gl": "^3.6.2",
    "next": "15.0.3",
    "next-pwa": "^5.6.0",
    "next-seo": "^6.4.0",
    "next-sitemap": "^4.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.48.2",
    "react-leaflet": "^4.2.1",
    "react-use": "^17.4.2",
    "sharp": "^0.33.1",
    "supercluster": "^8.0.1",
    "tailwind-merge": "^2.0.0",
    "workbox-webpack-plugin": "^7.0.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@lhci/cli": "^0.12.0",
    "@next/bundle-analyzer": "^15.0.3",
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20.9.2",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "autoprefixer": "^10.4.16",
    "critters": "^0.0.23",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.1",
    "eslint": "^8.54.0",
    "eslint-config-next": "15.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^11.4.0",
    "lint-staged": "^15.1.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "supabase": "^1.115.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "emergency",
    "coordination",
    "pwa",
    "offline",
    "maps",
    "supabase",
    "nextjs",
    "react",
    "typescript"
  ],
  "author": "OpenRelief Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/openrelief/openrelief.git"
  },
  "bugs": {
    "url": "https://github.com/openrelief/openrelief/issues"
  },
  "homepage": "https://openrelief.org"
}
