# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Database
DATABASE_URL=postgresql://postgres:password@localhost:54322/postgres

# Edge Functions
EDGE_FUNCTION_URL=https://your-project.supabase.co/functions/v1

# Maps
NEXT_PUBLIC_MAPTILER_API_KEY=your-maptiler-api-key
NEXT_PUBLIC_MAP_STYLE_URL=https://api.maptiler.com/maps/streets-v2/style.json

# Notifications
FCM_SERVER_KEY=your-fcm-server-key
APNS_KEY_ID=your-apns-key-id
APNS_TEAM_ID=your-apple-team-id

# Monitoring
SENTRY_DSN=your-sentry-dsn
NEXT_PUBLIC_SENTRY_DSN=your-public-sentry-dsn

# AI/ML
OPENAI_API_KEY=your-openai-api-key

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_OFFLINE_SYNC=true
NEXT_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true
NEXT_PUBLIC_ENABLE_TRUST_SYSTEM=true

# Environment
NEXT_PUBLIC_ENVIRONMENT=development
NODE_ENV=development

# Security
NEXTAUTH_SECRET=your-nextauth-secret
NEXTAUTH_URL=http://localhost:3000

# Performance
NEXT_PUBLIC_BUNDLE_ANALYZER=false

# Development
NEXT_PUBLIC_DEBUG_MODE=false